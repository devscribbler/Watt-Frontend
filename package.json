{
  "private": true,
  "name": "watt",
  "homepage": "https://github.com/WattUtilities/portal",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prettier-fix": "prettier -w .",
    "prettier-fix-staged": "pretty-quick --staged",
    "prettier-check": "prettier --check .",
    "precommit:format": "prettier-fix-staged",
    "storybook": "yarn workspace @watt/storybook storybook",
    "build-storybook": "yarn workspace @watt/storybook build-storybook",
    "test": "jest",
    "portal": "yarn workspace @watt/portal dev",
    "quote": "yarn workspace @watt/quote dev",
    "build-portal": "yarn workspace @watt/portal build",
    "build-quote": "yarn workspace @watt/quote build",
    "lhci-portal": "yarn workspace @watt/portal lhci",
    "lhci-quote": "yarn workspace @watt/quote lhci",
    "git-is-clean": "git diff-index --quiet HEAD",
    "lint": "eslint .",
    "cypress:run": "yarn workspace @watt/quote cypress:run",
    "cypress:open": "yarn workspace @watt/quote cypress:open"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.16.7",
    "@lhci/cli": "^0.12.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^29.4.3",
    "babel-loader": "^8.2.4",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "react-ga": "^3.3.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/open-sans": "^4.5.14",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@mui/styles": "^5.13.2",
    "@nivo/bar": "^0.79.1",
    "@nivo/core": "^0.79.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "axios": "^0.21.1",
    "emotion-theming": "^11.0.0",
    "next": "^12.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-redux": "^7.2.9",
    "react-use": "^17.3.2",
    "redux-persist": "^6.0.0"
  },
  "resolutions": {
    "@types/react": "^17.0.44",
    "@types/react-dom": "17.0.15"
  }
}
